{
    "name" : "repo",
    "jndiName" : "",
    "workspaces" : {
        "predefined" : ["fedora"],
        "default" : "fedora",
        "allowCreation" : true
    },
  "externalSources" : {
    "fileSystem" : {
      "classname" : "org.modeshape.connector.filesystem.FileSystemConnector",
      "directoryPath" : "target/test-classes/test-objects",
      "readonly" : false,
      "extraPropertiesStorage": "json",
      "cacheTtlSeconds" : 5,
      "projections" : [ "fedora:/federated => /" ]
     },
     "bagitFiles" : {
      "classname" : "org.fcrepo.federation.bagit.BagItConnector",
      "directoryPath" : "target/test-classes/test-bagit",
      "readonly" : false,
      "extraPropertiesStorage": "json",
      "cacheTtlSeconds" : 5,
      "projections" : [ "fedora:/federated => /" ]
     }
    },
    "security" : {
        "anonymous" : {
            "roles" : ["readonly","readwrite","admin"],
            "useOnFailedLogin" : false
        },
        "providers" : [
            { "classname" : "servlet" }
        ]
    },
"node-types" : ["fedora-node-types.cnd", "bagit-node-types.cnd"]
}