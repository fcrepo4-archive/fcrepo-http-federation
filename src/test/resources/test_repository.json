{
    "name" : "repo",
    "jndiName" : "",
    "workspaces" : {
        "predefined" : ["fedora"],
        "default" : "fedora",
        "allowCreation" : true
    },
    "externalSources" : {
        "filesystem-objects" : {
              "classname" : "org.modeshape.connector.filesystem.FileSystemConnector",
              "directoryPath" : "target/test-classes/test-objects",
              "readonly" : false,
              "extraPropertiesStorage": "json",
              "cacheTtlSeconds" : 5,
              "projections" : [ "fedora:/federated => /" ]
         }
    },
    "security" : {
        "anonymous" : {
            "roles" : ["readonly","readwrite","admin"],
            "useOnFailedLogin" : false
        },
        "providers" : [
            { "classname" : "servlet" }
        ]
    },
"node-types" : ["test-fedora-node-types.cnd", "test-bagit-node-types.cnd"]
}